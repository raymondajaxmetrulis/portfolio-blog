<html>
	<head>
        <meta charset="UTF-8">
        <link rel="stylesheet" href="/stylesheets/reset.css"/>
        <link rel="stylesheet" href="/stylesheets/main.css"/>
        <link rel="stylesheet" href="/stylesheets/blog.css"/>
  	</head>
  	<body>
    	<div class="blog">
    		<a href="/portfolio">
				<div class="link">
					<p>Portfolio</p>
				</div>
			</a>
    		<%  posts.forEach(function(posted) { 
    				let titles = posted.title;
    				let created = posted.created;
    				let bodies = posted.body;
    		%>
    		<div class="posts">
    			<h1><%= titles %></h1>
    			<h2>Posted on: <%= created %></h2>
    			<p><%= bodies %></p>
    			<div class="divider">
    			</div>
    		</div>
			<% }); %>
    	</div>
    </body>
</html>